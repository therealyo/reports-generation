{
  "name": "report-generation",
  "version": "0.0.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "build": "esbuild src/index.ts --platform=node --target=node18.4 --format=cjs --bundle --minify --main-fields=main --outfile=build/index.cjs  --external:pg-native",
    "start": "npm run build && node build/index.cjs",
    "sst:dev": "sst dev",
    "sst:build": "sst build",
    "sst:deploy": "sst deploy",
    "sst:remove": "sst remove",
    "sst:console": "sst console",
    "typecheck": "tsc --noEmit",
    "migration": "npx drizzle-kit generate:pg"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.316.0",
    "@aws-sdk/client-secrets-manager": "^3.316.0",
    "@sparticuz/chromium": "^112.0.2",
    "aws-sdk": "^2.1361.0",
    "axios": "^1.3.6",
    "drizzle-orm": "^0.23.13",
    "esbuild": "^0.17.17",
    "fast-xml-parser": "^4.2.0",
    "mailparser": "^3.6.4",
    "nodemailer": "^6.9.1",
    "pg": "^8.10.0",
    "puppeteer-core": "^19.9.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/aws-lambda": "^8.10.114",
    "@types/aws-sdk": "^2.7.0",
    "@types/axios": "^0.14.0",
    "@types/dotenv": "^8.2.0",
    "@types/mailparser": "^3.4.0",
    "@types/node": "^18.15.11",
    "@types/nodemailer": "^6.4.7",
    "@types/pg": "^8.6.6",
    "aws-cdk-lib": "2.72.1",
    "constructs": "10.1.156",
    "dotenv": "^16.0.3",
    "drizzle-kit": "^0.17.4",
    "sst": "^2.5.2",
    "typescript": "^5.0.4"
  },
  "workspaces": [
    "packages/*"
  ]
}
